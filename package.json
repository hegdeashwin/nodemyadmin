{
  "name": "nodemyadmin",
  "version": "0.0.1",
  "author": "Ashwin Hegde <ashwin.hegde3@gmail.com>",
  "description": "A tool intended to handle the administration of MySQL over the web for JavaScript developers",
  "contributors": [
    {
      "name": "Ashwin Hegde",
      "email": "ashwin.hegde3@gmail.com"
    }
  ],
  "scripts": {
    "start:server": "NODE_ENV=production babel-node ./server/server.js",
    "start:dev": "webpack-dev-server --config ./configs/build/webpack.config.js --progress --profile --watch --content-base src/",
    "start:dev:hot": "npm run start:dev -- --inline --hot",
    "dist:clean": "rimraf dist",
    "coverage:clean": "rimrag coverage",
    "bundle": "npm run dist:clean && webpack --config ./configs/build/webpack.config.js -d --watch",
    "build": "npm run dist:clean && webpack --config ./configs/build/webpack.config.js -p -d",
    "test:all": "karma start ./configs/test/karma.conf.js",
    "test:e2e": "protractor ./configs/test/protractor.conf.js",
    "postinstall": "typings install && npm run build",
    "lint:client": "tslint --config ./tslint.json \"src/**/*.ts\"",
    "lint:client:fix": "tslint --config ./tslint.json --fix \"src/**/*.ts\"",
    "lesslint": "./node_modules/.bin/lesshint src/app/",
    "webdriver:manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver:manager start",
    "webdriver:update": "npm run webdriver:manager update",
    "webdriver:status": "npm run webdriver:manager status",
    "webdriver:clean": "npm run webdriver:manager clean"
  },
  "pre-commit": {
    "run": [
      "lint:client"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/nodemyadmin/nodemyadmin"
  },
  "issues": {
    "url": "http://github.com/nodemyadmin/nodemyadmin/issues?state=open"
  },
  "milestones": {
    "url": "http://github.com/nodemyadmin/nodemyadmin/issues/milestones"
  },
  "releases": {
    "url": "http://github.com/nodemyadmin/nodemyadmin/releases"
  },
  "wiki": {
    "url": "http://github.com/nodemyadmin/nodemyadmin/wiki"
  },
  "dependencies": {
    "@angular/common": "^2.1.1",
    "@angular/compiler": "^2.1.1",
    "@angular/core": "^2.1.1",
    "@angular/forms": "^2.1.1",
    "@angular/http": "^2.1.1",
    "@angular/platform-browser": "^2.1.1",
    "@angular/platform-browser-dynamic": "^2.1.1",
    "@angular/router": "^3.1.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.13",
    "@types/node": "0.0.2",
    "bootstrap": "^4.0.0-alpha.5",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.6.1",
    "good": "^6.2.0",
    "good-console": "^5.0.2",
    "hapi": "^13.4.1",
    "inert": "^4.0.0",
    "joi": "^6.5.0",
    "jsonwebtoken": "^5.4.1",
    "mysql": "^2.7.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "angular-mock": "^1.0.0",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "codelyzer": "^2.0.0-beta.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lesshint": "^2.0.2",
    "pre-commit": "^1.1.3",
    "protractor": "^4.0.11",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.0-dev.3",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.0.2"
  },
  "keywords": [
    "mysqladmin",
    "mysql",
    "nodemysql"
  ],
  "engines": {
    "node": ">=6.0.0",
    "npm": ">= 3"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://raw.github.com/nodemyadmin/nodemyadmin/master/LICENSE"
    }
  ]
}
