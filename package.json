  "scripts": {
    "start:server": "NODE_ENV=production babel-node ./server/server.js",
    "start:dev": "lite-server -c bs-config.json",
    "dist:clean": "rimraf dist",
    "coverage:clean": "rimrag coverage",
    "bundle": "npm run dist:clean && webpack -d --watch",
    "build": "npm run dist:clean && webpack -p -d",
    "test:all": "karma start karma.conf.js",
    "postinstall": "typings install",
    "lint:ts": "tslint -c tslint.json 'src/**/*.ts'",
